<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>START</tag>
        <one-of>
          <item> go </item>
          <item> starts </item>
          <item> star </item>
        </one-of>
      </item>
      <item>
        <tag>STOP</tag>
        <one-of>
          <item> stop </item>
          <item> stops </item>
          <item> stap </item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>